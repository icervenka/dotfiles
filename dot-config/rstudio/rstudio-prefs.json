{
    "restore_source_documents": false,
    "save_workspace": "never",
    "load_workspace": false,
    "initial_working_directory": "~",
    "remove_history_duplicates": true,
    "restore_last_project": false,
    "num_spaces_for_tab": 4,
    "auto_detect_indentation": true,
    "syntax_color_console": true,
    "global_theme": "alternate",
    "editor_theme": "catppuccin-mocha",
    "posix_terminal_shell": "zsh",
    "panes": {
        "quadrants": [
            "Source",
            "Console",
            "TabSet1",
            "TabSet2"
        ],
        "tabSet1": [
            "Environment",
            "History",
            "Connections",
            "Build",
            "VCS",
            "Presentation"
        ],
        "tabSet2": [
            "Files",
            "Plots",
            "Packages",
            "Help",
            "Viewer",
            "Presentations"
        ],
        "hiddenTabSet": [
            "Tutorial"
        ],
        "console_left_on_top": false,
        "console_right_on_top": true,
        "additional_source_columns": 0
    },
    "default_project_location": "~",
    "jobs_tab_visibility": "shown",
    "new_proj_use_renv": true,
    "rmd_chunk_output_inline": false,
    "latex_preview_on_cursor_idle": "never",
    "knit_working_dir": "current"
}